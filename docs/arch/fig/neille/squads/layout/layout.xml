<?xml version="1.0" encoding="utf-8"?>
<layout xmlns="http://sina.khakbaz.com/2010/neille/ns/layout"
	width="8" height="5" default-width="1" default-height="1"
	onbx="108" onby="150">
  <row height="1.5">
    <col id="detail" model="deck" width="1.5"/>
    <col width="0.5"/>
    <col id="hero" model="deck" width="1.5"/>
    <col id="units" model="cardlist" width="6">
      <row>
	<col id="unit-a"/>
	<col id="unit-b"/>
	<col id="unit-c"/>
	<col id="unit-d"/>
	<col id="unit-e"/>
	<col id="unit-f"/>
      </row>
    </col>
  </row>
  <row height="0.4"/>
  <row height="0.2">
    <col id="prev" width="0.4" model="deck"/>
    <col width="0.2"/>
    <col id="next" width="0.4" model="deck"/>
  </row>
  <row height="0.2"/>
  <row height="1">
    <col id="cardlist" width="9">
      <row>
	<col id="card-a"/>
	<col id="card-b"/>
	<col id="card-c"/>
	<col id="card-d"/>
	<col id="card-e"/>
	<col id="card-f"/>
	<col id="card-g"/>
	<col id="card-h"/>
	<col id="card-i"/>
      </row>
    </col>
  </row>
</layout>
